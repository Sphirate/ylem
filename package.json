{
  "name": "ylem",
  "version": "0.1.0",
  "private": true,
  "description": "State managment tool for JavaScript apps",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./lib ./types ./coverage",
    "test": "./node_modules/.bin/jest.cmd",
    "coverage": "./node_modules/.bin/jest.cmd --coverage",
    "prebuild": "npm run clean",
    "build": "./node_modules/.bin/rollup.cmd --config ./rollup.config.js"
  },
  "keywords": [
    "javascript",
    "state-managment"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Sphirate/ylem.git"
  },
  "author": "Dmitry Rykov <sphirate@gmail.com>",
  "files": [
    "lib",
    "types"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/jest": "^24.0.23",
    "jest": "^24.9.0",
    "react": "^16.12.0",
    "rollup": "^1.27.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.25.2",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^24.1.0",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@types/react": "^16.9.17"
  },
  "jest": {
    "testRegex": "(/test/.*\\.spec\\.[tj]s)$"
  }
